<div class="item col-xs-6 col-sm-6 col-md-4 col-lg-3 column">
	<img src="[image]" class="img-responsive">
	<div class="producttitle">[producttitle]</div>
	<div class="type">[type]</div>
	<div class="productprice">
	<div class="pull-right">
		<button data-toggle="fade bindData" data-value="[uid]" frame-target="#Shop" group-target="#Repair" class="btn btn-danger btn-sm" role="button">BUY</button></div>
		<div class="pricetext">[productprice]</div>
	</div>
</div>