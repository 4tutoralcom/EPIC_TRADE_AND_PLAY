<div class="item col-xs-12 col-sm-6 col-md-4 column">
	<img src="[image]" class="img-responsive productpicture">
	<div class="producttitle">[name]</div>
	<div class="description">[description]</div>
	<div id="uid" class="hidden">[uid]</div>
	<div class="productprice">
		<div class="pull-right">
			<button 
				data-toggle="fade bindData"
				data-value='{"#Contact #tittle":"/i/[producttitle] : [name]", "#ContactUs #email":"/v/[email]", "#ContactUs #name":"/v/[FirstName] [LastName]"}' frame-target="#Shop" group-target="#Contact" 
				class="btn btn-block btn-danger btn-sm" 
				role="button">Contact us
			</button>
		</div>
		<div class="pricetext">[productprice]</div>
	</div>
</div>